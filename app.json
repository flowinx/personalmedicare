    "scheme": "personalmedicare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.personalmedicare",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar suas fotos para permitir que vocÃª escolha uma imagem de perfil."
      }
